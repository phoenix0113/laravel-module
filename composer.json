{
  "name": "nwidart/laravel-modules",
  "description": "Laravel Module management",
  "keywords": [
    "modules",
    "laravel",
    "nwidart",
    "module",
    "rad"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com",
      "homepage": "https://nicolaswidart.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "laravel/framework": "5.1.*|5.2.*",
    "laravelcollective/html": "5.1.*|5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "mockery/mockery": "~0.9",
    "orchestra/testbench": "^3.1|^3.2",
    "satooshi/php-coveralls": "~1.0",
    "phpunit/phpcov": "2.*",
    "phpro/grumphp": "^0.9.1",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Nwidart\\Modules\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nwidart\\Modules\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
